{
  "name": "cozinhadosbonitos-studio",
  "private": true,
  "version": "1.0.0",
  "description": "Sanity studio for Cozinha dos Bonitos",
  "main": "package.json",
  "author": "Bruno Alla <alla.brunoo@gmail.com>",
  "license": "MIT",
  "scripts": {
    "format": "prettier --write .",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier --check .",
    "lint:tsc": "tsc",
    "deploy": "npm run deploy:studio && npm run deploy:graphql",
    "deploy:studio": "sanity deploy",
    "deploy:graphql": "sanity graphql deploy --force",
    "dev": "sanity dev",
    "start": "sanity start",
    "build": "sanity build"
  },
  "keywords": [
    "sanity"
  ],
  "dependencies": {
    "@sanity/vision": "^3.0.6",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-icons": "5.5.0",
    "sanity": "3.77.0",
    "styled-components": "6.1.15",
    "typescript": "5.7.3"
  },
  "devDependencies": {
    "@eslint/compat": "1.2.7",
    "@eslint/eslintrc": "3.3.0",
    "@eslint/js": "9.21.0",
    "@typescript-eslint/parser": "8.24.1",
    "eslint": "9.21.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.2.3",
    "eslint-plugin-react": "7.37.4",
    "globals": "15.15.0",
    "prettier": "3.5.2"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "renovate": {
    "extends": [
      "github>browniebroke/renovate-configs",
      "github>browniebroke/renovate-configs:eslint-prettier"
    ]
  }
}
